// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "explorerViewletVisible"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusRightGroup",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+e",
        "command": "revealInExplorer"
    },
    {
        "key": "ctrl+e",
        "command": "workbench.view.explorer",
        "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "ctrl+e",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "explorerViewletVisible"
    },
    {
        "key": "ctrl+e",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "explorerViewletVisible"
    },
    {
        "key": "a",
        "command": "explorer.newFile",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "shift+a",
        "command": "explorer.newFolder",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "r",
        "command": "renameFile",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "d",
        "command": "deleteFile",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "c",
        "command": "filesExplorer.copy",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "x",
        "command": "filesExplorer.cut",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "p",
        "command": "filesExplorer.paste",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "f",
        "command": "filesExplorer.findInFolder",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "w",
        "command": "workbench.files.action.collapseExplorerFolders",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "o",
        "command": "list.select",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "ctrl+v",
        "command": "explorer.openToSide",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "ctrl+v",
        "command": "workbench.action.alternativeAcceptSelectedQuickOpenItem",
        "when": "inQuickOpen"
    },
    {
        "key": "y",
        "command": "copyRelativeFilePath",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "shift+k",
        "command": "editor.action.showPreviewHover",
        "when": "editorFocus && !inputFocus && !editorHasSelection"
    },
    {
        "key": "shift+y",
        "command": "copyFilePath",
        "when": "explorerViewletFocus && !inputFocus"
    },
    {
        "key": "ctrl+g",
        "command": "workbench.view.scm"
    },
    {
        "key": "ctrl+g",
        "command": "-workbench.view.scm"
    },
    {
        "key": "ctrl+g",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "activeViewlet == 'workbench.view.scm'"
    },
    {
        "key": "ctrl+g",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "activeViewlet == 'workbench.view.scm'"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "activeViewlet == 'workbench.view.scm'"
    },
    {
        "key": "ctrl+shift+d",
        "command": "workbench.view.debug"
    },
    {
        "key": "ctrl+shift+d",
        "command": "-workbench.view.debug"
    },
    {
        "key": "ctrl+shift+d",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "activeViewlet == 'workbench.view.debug'"
    },
    {
        "key": "ctrl+shift+d",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "activeViewlet == 'workbench.view.debug'"
    },
    {
        "key": "ctrl+x",
        "command": "workbench.view.extensions"
    },
    {
        "key": "ctrl+x",
        "command": "-workbench.view.extensions"
    },
    {
        "key": "ctrl+x",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "activeViewlet == 'workbench.view.extensions'"
    },
    {
        "key": "ctrl+x",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "activeViewlet == 'workbench.view.extensions'"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.togglePanel",
        "when": "panelFocus"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.focusPanel",
        "when": ""
    },
    {
        "key": "ctrl+i",
        "command": "workbench.action.navigateForward",
        "when": "canNavigateForward"
    },
    {
        "key": "ctrl+shift+-",
        "command": "-workbench.action.navigateForward",
        "when": "canNavigateForward"
    },
    {
        "key": "ctrl+o",
        "command": "workbench.action.navigateBack",
        "when": "canNavigateBack"
    },
    {
        "key": "ctrl+-",
        "command": "-workbench.action.navigateBack",
        "when": "canNavigateBack"
    },
]